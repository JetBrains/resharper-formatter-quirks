<?xml version="1.0" encoding="utf-8"?>
<!-- See the :pluginNugetPack task in ../rider-plugin/build.gradle.kts for variables sources -->
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>JetBrains.ReSharper.Plugins.FormattingQuirks</id>
    <title>Quirky Formattings</title>
    <version>$plugin_version$</version>
    <authors>JetBrains</authors>
    <owners>JetBrains</owners>
    <summary>A plugin that accumulates some of the *quirkier* formatting rules.</summary>
    <description>These rules didn't fit quite nicely into the full ReSharper ruleset, but are presented here now instead.</description>
    <releaseNotes>$ReleaseNotes$</releaseNotes>
    <iconUrl>http://resharper-plugins.jetbrains.com/Content/Images/packageReSharper.png</iconUrl>
    <copyright>Copyright 2022 JetBrains, s.r.o</copyright>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
	<projectUrl>https://github.com/JetBrains/resharper-formatter-quirks</projectUrl>
    <licenseUrl>https://raw.githubusercontent.com/JetBrains/resharper-formatter-quirks/blob/net$wave_id$/license.txt</licenseUrl>
    <dependencies>
      <dependency id="Wave" version="[$wave_id$.0.0]" />
    </dependencies>
    <tags>resharper formatting</tags>
  </metadata>
  <files>
    <file src="QuirkyFormattings\bin\$configuration$\$dotnet_version$\JetBrains.ReSharper.Plugins.QuirkyFormattings.dll" target="DotFiles" />
    <file src="QuirkyFormattings\bin\$configuration$\$dotnet_version$\JetBrains.ReSharper.Plugins.QuirkyFormattings.pdb" target="DotFiles" />
  </files>
</package>